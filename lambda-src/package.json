{
  "scripts": {
    "clean": "rm -r dist .cache",
    "fmt": "prettier --write '**/*.{ts,json}'",
    "lint": "eslint '**/*.ts'",
    "lint:fix": "eslint --fix '**/*.ts'",
    "test": "jest",
    "build": "tsc --noEmit && parcel build --target node --no-minify --bundle-node-modules --no-source-maps",
    "build:client": "yarn build ./src/client.ts",
    "start:client": " yarn build:client && node dist/client.js",
    "build:lambda": "yarn build -o lambda/bundle src/presentation/handler/index.ts",
    "init:lambroll": "lambroll init --region ap-northeast-1 --function-name",
    "logs": "lambroll logs --region ap-northeast-1",
    "deploy": "yarn build:lambda && lambroll deploy --region ap-northeast-1 --src ./dist/lambda"
  },
  "devDependencies": {
    "@types/aws-lambda": "^8.10.56",
    "@types/axios": "^0.14.0",
    "@types/dotenv": "^8.2.0",
    "@types/faker": "^4.1.12",
    "@types/file-type": "^10.9.1",
    "@types/jest": "^26.0.3",
    "@types/js-base64": "^2.3.2",
    "@types/node": "^14.0.13",
    "@types/validator": "^13.1.0",
    "@typescript-eslint/eslint-plugin": "^3.5.0",
    "@typescript-eslint/parser": "^3.5.0",
    "eslint": "^7.4.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "faker": "^4.1.0",
    "jest": "^26.1.0",
    "parcel-bundler": "^1.12.4",
    "prettier": "^2.0.5",
    "ts-jest": "^26.1.1",
    "ts-mockito": "^2.6.1",
    "ts-node": "^8.10.2",
    "ts-node-paths": "^1.0.1",
    "tsconfig-paths": "^3.9.0",
    "typescript": "^3.9.5"
  },
  "dependencies": {
    "aws-lambda": "^1.0.6",
    "aws-sdk": "^2.706.0",
    "axios": "^0.21.1",
    "dayjs": "^1.8.29",
    "dotenv": "^8.2.0",
    "file-type": "^14.6.2",
    "node-base64-image": "^2.0.1",
    "querystring": "^0.2.0",
    "uuid": "^8.2.0",
    "validator": "^13.1.1"
  }
}
